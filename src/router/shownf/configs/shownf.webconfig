<fieldset>
	<legend>Show new Features<a name="wcshownf" id="wcshownf"></a></legend>
	<div class="setting">
		<div class="label">Display new Features</div>
		<input class="spaceradio" type="radio" value="1" name="shownf" <% nvc("shownf","1"); %> /><% tran("share.enable"); %>&nbsp;
		<input class="spaceradio" type="radio" value="0" name="shownf" <% nvc("shownf","0"); %> /><% tran("share.disable"); %>
	</div>
</fieldset><br/>
